<actions>
<action>
	<icon>catfish</icon>
	<name>Find files</name>
	<unique-id>1553284318141740-1</unique-id>
	<command>catfish --hidden %f</command>
	<description>Search for files with Catfish</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>utilities-terminal</icon>
	<name>Open terminal here</name>
	<unique-id>1553284318141749-2</unique-id>
	<command>sh -c &quot;cd %f; x-terminal-emulator&quot;</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>system-file-manager</icon>
	<name>Open as root</name>
	<unique-id>1553284318141754-3</unique-id>
	<command>pkexec thunar %f</command>
	<description>Open directory as root</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>accessories-text-editor</icon>
	<name>Edit as root</name>
	<unique-id>1553284318141761-4</unique-id>
	<command>pkexec x-text-editor %F</command>
	<description>Open text files as root</description>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>applications-system</icon>
	<name>Check sha256 sum</name>
	<unique-id>1553284318141767-5</unique-id>
	<command>yad --image=dialog-info --button=OK --title=&quot;Check sha256 for %n&quot; --text=&quot;$(sha256sum %f)&quot; --borders=20 --center</command>
	<description></description>
	<patterns>*.iso;*.img;*.sub</patterns>
	<other-files/>
</action>
</actions>
